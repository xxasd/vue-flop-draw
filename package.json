{
  "name": "vue-flop-draw",
  "version": "0.1.7",
  "description": "Vue version of the flop draw component",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xxasd/vue-flop-draw"
  },
  "license": "MIT",
  "main": "lib/vue-flop-draw.common.js",
